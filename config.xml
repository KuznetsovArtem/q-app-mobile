<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.theQapp" version="0.1.0 " xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>clientserviceukraineapp</name>
    <description>
        Q app
    </description>
    <author email="ksu.developer@gmail.com" href="http://cordova.io">
        ksu.developer
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="file://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
    <platform name="android">
      <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
    </platform>
</widget>
